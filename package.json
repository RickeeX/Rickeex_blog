{
  "name": "tailwind-nextjs-starter-blog",
  "version": "2.3.0",
  "private": true,
  "scripts": {
    "start": "next dev",
    "dev": "velite && cross-env INIT_CWD=$PWD next dev",
    "build": "velite && cross-env INIT_CWD=$PWD next build && cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/postbuild.mjs",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "eslint --fix app components lib layouts scripts",
    "prepare": "husky"
  },
  "dependencies": {
    "@fontsource-variable/hanken-grotesk": "^5.1.0",
    "@fontsource-variable/noto-sans-hk": "^5.1.0",
    "@fontsource-variable/noto-sans-sc": "^5.1.0",
    "@fontsource/noto-sans-hk": "^5.1.0",
    "@giscus/react": "^3.1.0",
    "@headlessui/react": "2.2.0",
    "@next/bundle-analyzer": "^16.1.0",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "autoprefixer": "^10.4.13",
    "body-scroll-lock": "^4.0.0-beta.0",
    "esbuild": "0.20.2",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.2",
    "hast-util-from-html-isomorphic": "^2.0.0",
    "image-size": "1.0.0",
    "kbar": "^0.1.0-beta.47",
    "next": "^16.1.1",
    "next-mdx-remote": "^5.0.0",
    "next-themes": "^0.4.6",
    "postcss": "^8.4.24",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-citation": "^2.0.0",
    "rehype-katex": "^7.0.0",
    "rehype-katex-notranslate": "^1.1.4",
    "rehype-preset-minify": "7.0.0",
    "rehype-prism-plus": "^2.0.0",
    "rehype-slug": "^6.0.0",
    "remark": "^15.0.0",
    "remark-gfm": "^4.0.0",
    "remark-github-blockquote-alert": "^1.2.1",
    "remark-math": "^6.0.0",
    "tailwindcss": "^3.4.17",
    "unist-util-visit": "^5.0.0",
    "velite": "^0.2.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.16.0",
    "@svgr/webpack": "^8.0.1",
    "@types/mdx": "^2.0.12",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.12.0",
    "@typescript-eslint/parser": "^8.12.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.14.0",
    "eslint-config-next": "^16.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^15.12.0",
    "husky": "^9.0.0",
    "lint-staged": "^13.0.0",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "typescript": "^5.1.3"
  },
  "overrides": {
    "kbar": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "@giscus/react": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  },
  "packageManager": "yarn@3.6.1"
}
